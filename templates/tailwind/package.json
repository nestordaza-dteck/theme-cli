{
    "name": "default-theme",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "scripts:build": "scripts-theme build",
        "scripts:start": "scripts-theme start",
        "start": "node -r dotenv/config main/ && cross-env NODE_ENV=development webpack serve --config-name browser --hot --open",
        "build": "node -r dotenv/config main/ && cross-env NODE_ENV=production webpack --mode production",
        "build:dev": "node -r dotenv/config main/ && cross-env NODE_ENV=development webpack --mode development",
        "build:ssr": "env NODE_ENV=development webpack --config-name server",
        "build:data": "node -r dotenv/config main/",
        "test2": "cross-env NODE_ENV=development webpack --config ./webpack.browser.js --mode production",
        "test": "cross-env NODE_ENV=development webpack --mode development"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-player": "^2.7.2",
        "react-router-dom": "^5.2.0",
        "typescript": "^4.2.4"
    },
    "devDependencies": {
        "@types/node": "^16.0.1",
        "@types/react-dom": "^17.0.9",
        "@types/react-router-dom": "^5.1.8"
    }
}
